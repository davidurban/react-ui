    "stylelint-config-visionapps-order": "^0.2.0",
    "stylelint-order": "^0.7.0",
    "stylelint-webpack-plugin": "^0.9.0",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.10.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.20.0",
    "prop-types": "^15.5.10",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  }
