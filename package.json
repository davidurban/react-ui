{
  "name": "react-ui",
  "version": "0.2.0",
  "license": "unlicensed",
  "main": "dist/lib.js",
  "scripts": {
    "build": "npm run lib:build && npm run demo:build",
    "demo:build": "webpack --config webpack.config.demo.js",
    "demo:start": "webpack-dev-server --config webpack.config.demo.js",
    "demo:test": "stylelint \"src/demo/**/*.scss\" \"demo/css/*.css\" --config stylelint.config.js && eslint --ext js,jsx src/demo",
    "lib:build": "webpack --config webpack.config.lib.js",
    "lib:test": "stylelint \"src/lib/**/*.scss\" --config stylelint.config.js && eslint --ext js,jsx src/lib && jest -w",
    "prepublish": "npm run lib:build",
    "start": "npm run demo:start",
    "test": "npm run lib:test && npm run demo:test"
  },
  "jest": {
    "verbose": true,
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleNameMapper": {
      "\\.(jpg|svg)$": "<rootDir>/tests/mocks/emptyMock.js",
      "\\.(scss)$": "identity-obj-proxy"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/test/setup.js"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.6",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.17.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.24.1",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "eslint": "^4.19.1",
    "eslint-config-visionapps": "^0.1.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^0.10.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^20.0.4",
    "json-loader": "^0.5.7",
    "material-design-icons": "^3.0.1",
    "node-sass": "^4.8.3",
    "postcss-loader": "^2.1.3",
    "sass-loader": "^6.0.7",
    "sinon": "^3.1.0",
    "style-loader": "^0.18.1",
    "stylelint": "^8.0.0",
    "stylelint-config-visionapps": "^0.4.0",
    "stylelint-config-visionapps-order": "^0.2.0",
    "stylelint-order": "^0.7.0",
    "stylelint-webpack-plugin": "^0.9.0",
    "svg-sprite-loader": "^3.7.3",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.2"
  },
  "dependencies": {
    "babel-polyfill": "^6.20.0",
    "prop-types": "^15.6.1",
    "react": "^16.3.1",
    "react-dom": "^16.3.1"
  }
}
